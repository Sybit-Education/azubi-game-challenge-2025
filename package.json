{
  "name": "azubi-game-challenge-2025",
  "description": "A Phaser 3 TypeScript Game.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sybit-Education/azubi-game-challenge-2025.git"
  },
  "author": "Sybit GmbH",
