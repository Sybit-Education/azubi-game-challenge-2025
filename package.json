    "eslint": "^8.56.0",
    "eslint-plugin-phaser": "^0.0.3",
    "typescript": "~5.9.2",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "phaser": "^3.90.0",
    "terser": "^5.43.1",
    "tsx": "^4.20.5"
  }
